{
  "name": "Aleatory",
  "version": "0.1.1",
  "manifest_version": 2,
  "description": "Listen to your browser make requests.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png"
  },
  "background": {
    "scripts": ["util.js", "functions.js", "charlie.js"]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*/*"
  ]
}
